<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="admin">
    <select id="selectAdmin" parameterType="Project.pro.gg.Model.AdminDTO" resultType="Project.pro.gg.Model.AdminDTO">
        select * from admin
        where adminId = #{adminId}
    </select>

    <select id="selectOne" parameterType="String" resultType="Project.pro.gg.Model.MemberDTO">
        select * from member
        where userid = #{userid}
    </select>

    <select id="selectByNickname" parameterType="String" resultType="Project.pro.gg.Model.MemberDTO">
        select * from member
        where nickname = #{nickname}
    </select>
</mapper>